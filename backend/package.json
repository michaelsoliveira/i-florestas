{
  "name": "@bomanejo/backend",
  "version": "1.24.4-alpha.26",
  "description": "BOManejo",
  "main": "./src/app.ts",
  "scripts": {
    "start": "nodemon --exec \"ts-node\" --files \"./src/app.ts\"",
    "dev": "nodemon --watch './**/*.ts' --exec \"ts-node\" --files \"./src/app.ts\"",
    "migrate:prod": "npx prisma migrate deploy && npm run dev",
    "docker:start": "docker-compose up -d",
    "build": "docker-compose build",
    "stop": "docker-compose down",
    "clean": "npm run clean:images && npm run clean:volumes",
    "clean:images": "docker system prune -af",
    "clean:volumes": "docker volume prune -f",
    "generate": "docker exec -it backend npx prisma generate",
    "migrate": "docker exec -it backend npx prisma migrate dev",
    "seed": "docker exec -it backend npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "Michael Oliveira <michaelsoliveira@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@sentry/node": "^7.99.0",
    "@sentry/profiling-node": "^1.3.5",
    "@sentry/tracing": "^7.99.0",
    "@types/jsonwebtoken": "^9.0.5",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "consign": "^0.1.6",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "dayjs": "^1.11.10",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "googleapis": "^131.0.0",
    "jwt-simple": "^0.5.6",
    "mathjs": "^12.3.0",
    "matomo-tracker": "^2.2.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.1",
    "nodemon": "^3.0.3",
    "oauth": "^0.9.15",
    "oauth-signature": "^1.5.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.11.2",
    "prisma": "^5.9.0",
    "request": "^2.88.2",
    "sequelize": "^6.32.1",
    "socket.io": "^4.7.4",
    "tld-extract": "^2.0.1",
    "ts-node": "^10.9.2",
    "unix-timestamp": "^1.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.15",
    "@types/nodemailer": "^6.4.14",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1"
  }
}
